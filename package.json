{
  "name": "Firekylin",
  "description": "A Simple & Fast Node Bloging Platform Base On ThinkJS 2.0 & ReactJS & ES2015+",
  "version": "0.15.6",
  "scripts": {
    "start": "node www/development.js",
    "compile": "babel --presets es2015,stage-1 --plugins transform-runtime src/ --out-dir app/",
    "watch-compile": "node -e \"console.log('<npm run watch-compile> no longer need, use <npm start> command directly.');console.log();\"",
    "watch": "npm run watch-compile",
    "copy-package": "node bin/copy_package.js",
    "webpack": "webpack --watch",
    "webpack.build": "webpack",
    "webpack.build.production": "webpack --config=webpack.production.config.babel.js",
    "lint": "eslint ./src ./*.js ./www/*.js ./www/static/src ./www/theme/firekylin --ignore-path .gitignore --cache -f table --ext .js,.jsx || true",
    "lint-fix": "eslint ./src ./*.js ./www/*.js ./www/static/src ./www/theme/firekylin --ignore-path .gitignore -f table --ext .js,.jsx --fix || true",
    "lint-break-on-errors": "echo '\n[INFO] Running ESLint...\n[INFO] (Autofix with `npm run lint-fix`)' && eslint ./src ./*.js ./www/*.js ./www/static/src ./www/theme/firekylin --ignore-path .gitignore -f table --ext .js,.jsx",
    "check-extraneous-pkgs": "npm list --depth=0 && echo '[SUCCESS] No extraneous packages.' || (echo '[INFO] Pruning extraneous packages...\n' && npm prune)"
  },
  "dependencies": {
    "babel-runtime": "6.x.x",
    "highlight.js": "^9.2.0",
    "markdown-toc": "^1.0.2",
    "marked": "^0.3.5",
    "moment": "^2.11.2",
    "node-crontab": "0.0.8",
    "nodemailer": "^2.3.2",
    "nunjucks": "^2.3.0",
    "phpass": "^0.1.1",
    "push-to-firekylin": "^0.2.2",
    "qiniu": "^6.1.13",
    "request": "^2.69.0",
    "semver": "^5.1.0",
    "source-map-support": "^0.4.0",
    "speakeasy": "2.0.0",
    "thinkjs": "^2.1.5",
    "to-markdown": "^3.0.3",
    "upyun": "^2.0.4",
    "xml2js": "^0.4.16"
  },
  "devDependencies": {
    "autobind-decorator": "^1.3.3",
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "6.x.x",
    "babel-preset-stage-1": "^6.5.0",
    "classnames": "^2.2.3",
    "codemirror": "^5.24.2",
    "css-loader": "^0.23.1",
    "eslint": "^3.19.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "history": "^1.17.0",
    "md5": "^2.0.0",
    "pre-commit": "^1.2.2",
    "qrcode-react": "^0.1.10",
    "rc-select": "^5.10.0",
    "react": "^0.14.7",
    "react-bootstrap": "^0.28.3",
    "react-bootstrap-validation": "^0.1.11",
    "react-codemirror": "^0.3.0",
    "react-color": "^2.1.0",
    "react-datetime": "^2.0.3",
    "react-dom": "^0.14.7",
    "react-mixin": "^3.0.3",
    "react-router": "^2.0.0-rc5",
    "react-ui-tree": "^2.6.0",
    "reflux": "^0.3.0",
    "stc": "^2.0.2",
    "stc-css-combine": "^1.0.1",
    "stc-css-compress": "^1.0.1",
    "stc-html-compress": "^1.0.1",
    "stc-localstorage": "^1.0.3",
    "stc-localstorage-nunjucks": "^1.0.1",
    "stc-resource-version": "^1.0.0",
    "stc-uglify": "^1.0.1",
    "style-loader": "^0.13.0",
    "superagent": "^1.7.2",
    "webpack": "^2.2.0"
  },
  "repository": "https://github.com/firekylin/firekylin",
  "license": "MIT",
  "pre-commit": [
    "check-extraneous-pkgs",
    "lint-break-on-errors"
  ]
}
